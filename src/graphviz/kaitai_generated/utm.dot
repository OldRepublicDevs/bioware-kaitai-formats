digraph {
	rankdir=LR;
	node [shape=plaintext];
	subgraph cluster__utm {
		label="Utm";
		graph[style=dotted];

		utm__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
			<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
			<TR><TD PORT="gff_data_pos">0</TD><TD PORT="gff_data_size">56</TD><TD>Gff::Gff</TD><TD PORT="gff_data_type">gff_data</TD></TR>
		</TABLE>>];
		utm__inst__file_type_valid [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
			<TR><TD BGCOLOR="#E0FFE0">id</TD><TD BGCOLOR="#E0FFE0">value</TD></TR>
			<TR><TD>file_type_valid</TD><TD>gff_data.header.file_type == &quot;UTM &quot;</TD></TR>
		</TABLE>>];
		utm__inst__version_valid [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
			<TR><TD BGCOLOR="#E0FFE0">id</TD><TD BGCOLOR="#E0FFE0">value</TD></TR>
			<TR><TD>version_valid</TD><TD> ((gff_data.header.file_version == &quot;V3.2&quot;) || (gff_data.header.file_version == &quot;V3.3&quot;)) </TD></TR>
		</TABLE>>];
	}
	utm__seq:gff_data_type -> gff__seq [style=bold];
	gff_header__seq:file_type_type -> utm__inst__file_type_valid [color="#404040"];
	gff_header__seq:file_version_type -> utm__inst__version_valid [color="#404040"];
}
