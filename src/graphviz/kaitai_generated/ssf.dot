			<TR><TD PORT="sounds_pos">sounds_offset</TD><TD PORT="sounds_size">112</TD><TD>SoundArray</TD><TD PORT="sounds_type">sounds</TD></TR>
		</TABLE>>];
		subgraph cluster__padding {
			label="Ssf::Padding";
			graph[style=dotted];

			padding__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="padding_bytes_pos">0</TD><TD PORT="padding_bytes_size">4</TD><TD>u4le</TD><TD PORT="padding_bytes_type">padding_bytes</TD></TR>
