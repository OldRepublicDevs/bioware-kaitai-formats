digraph {
	rankdir=LR;
	node [shape=plaintext];
	subgraph cluster__ncs_minimal {
		label="NcsMinimal";
		graph[style=dotted];

		ncs_minimal__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
			<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
			<TR><TD PORT="file_type_pos">0</TD><TD PORT="file_type_size">4</TD><TD>str(ASCII)</TD><TD PORT="file_type_type">file_type</TD></TR>
			<TR><TD PORT="file_version_pos">4</TD><TD PORT="file_version_size">4</TD><TD>str(ASCII)</TD><TD PORT="file_version_type">file_version</TD></TR>
			<TR><TD PORT="size_marker_pos">8</TD><TD PORT="size_marker_size">1</TD><TD>u1</TD><TD PORT="size_marker_type">size_marker</TD></TR>
			<TR><TD PORT="total_file_size_pos">9</TD><TD PORT="total_file_size_size">4</TD><TD>u4be</TD><TD PORT="total_file_size_type">total_file_size</TD></TR>
			<TR><TD PORT="instructions_pos">13</TD><TD PORT="instructions_size">2</TD><TD>Instruction</TD><TD PORT="instructions_type">instructions</TD></TR>
			<TR><TD COLSPAN="4" PORT="instructions__repeat">repeat until _io.pos &gt;= _root.total_file_size</TD></TR>
		</TABLE>>];
		subgraph cluster__instruction {
			label="NcsMinimal::Instruction";
			graph[style=dotted];

			instruction__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="bytecode_pos">0</TD><TD PORT="bytecode_size">1</TD><TD>u1</TD><TD PORT="bytecode_type">bytecode</TD></TR>
				<TR><TD PORT="qualifier_pos">1</TD><TD PORT="qualifier_size">1</TD><TD>u1</TD><TD PORT="qualifier_type">qualifier</TD></TR>
			</TABLE>>];
		}
	}
	ncs_minimal__seq:instructions_type -> instruction__seq [style=bold];
	ncs_minimal__seq:total_file_size_type -> ncs_minimal__seq:instructions__repeat [color="#404040"];
}
