digraph {
	rankdir=LR;
	node [shape=plaintext];
	subgraph cluster__rim {
		label="Rim";
		graph[style=dotted];

		rim__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
			<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
			<TR><TD PORT="header_pos">0</TD><TD PORT="header_size">20</TD><TD>RimHeader</TD><TD PORT="header_type">header</TD></TR>
			<TR><TD PORT="extended_header_pos">20</TD><TD PORT="extended_header_size">100</TD><TD>RimExtendedHeader</TD><TD PORT="extended_header_type">extended_header</TD></TR>
			<TR><TD PORT="resource_entry_table_pos">120</TD><TD PORT="resource_entry_table_size">...</TD><TD>ResourceEntryTable</TD><TD PORT="resource_entry_table_type">resource_entry_table</TD></TR>
			<TR><TD COLSPAN="4" PORT="resource_entry_table__if">if header.resource_count &gt; 0</TD></TR>
		</TABLE>>];
		subgraph cluster__resource_entry {
			label="Rim::ResourceEntry";
			graph[style=dotted];

			resource_entry__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="resref_pos">0</TD><TD PORT="resref_size">16</TD><TD>str(ASCII)</TD><TD PORT="resref_type">resref</TD></TR>
				<TR><TD PORT="resource_type_pos">16</TD><TD PORT="resource_type_size">4</TD><TD>u4leâ†’XoreosFileTypeId</TD><TD PORT="resource_type_type">resource_type</TD></TR>
				<TR><TD PORT="resource_id_pos">20</TD><TD PORT="resource_id_size">4</TD><TD>u4le</TD><TD PORT="resource_id_type">resource_id</TD></TR>
				<TR><TD PORT="offset_to_data_pos">24</TD><TD PORT="offset_to_data_size">4</TD><TD>u4le</TD><TD PORT="offset_to_data_type">offset_to_data</TD></TR>
				<TR><TD PORT="resource_size_pos">28</TD><TD PORT="resource_size_size">4</TD><TD>u4le</TD><TD PORT="resource_size_type">resource_size</TD></TR>
			</TABLE>>];
			resource_entry__inst__data [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="data_pos">offset_to_data</TD><TD PORT="data_size">1</TD><TD>u1</TD><TD PORT="data_type">data</TD></TR>
				<TR><TD COLSPAN="4" PORT="data__repeat">repeat resource_size times</TD></TR>
			</TABLE>>];
		}
		subgraph cluster__resource_entry_table {
			label="Rim::ResourceEntryTable";
			graph[style=dotted];

			resource_entry_table__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="entries_pos">0</TD><TD PORT="entries_size">32</TD><TD>ResourceEntry</TD><TD PORT="entries_type">entries</TD></TR>
				<TR><TD COLSPAN="4" PORT="entries__repeat">repeat _root.header.resource_count times</TD></TR>
			</TABLE>>];
		}
		subgraph cluster__rim_extended_header {
			label="Rim::RimExtendedHeader";
			graph[style=dotted];

			rim_extended_header__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="reserved_padding_pos">0</TD><TD PORT="reserved_padding_size">100</TD><TD>str(ASCII)</TD><TD PORT="reserved_padding_type">reserved_padding</TD></TR>
			</TABLE>>];
		}
		subgraph cluster__rim_header {
			label="Rim::RimHeader";
			graph[style=dotted];

			rim_header__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="file_type_pos">0</TD><TD PORT="file_type_size">4</TD><TD>str(ASCII)</TD><TD PORT="file_type_type">file_type</TD></TR>
				<TR><TD COLSPAN="4" PORT="file_type__valid">must be equal to &quot;RIM &quot;</TD></TR>
				<TR><TD PORT="file_version_pos">4</TD><TD PORT="file_version_size">4</TD><TD>str(ASCII)</TD><TD PORT="file_version_type">file_version</TD></TR>
				<TR><TD COLSPAN="4" PORT="file_version__valid">must be equal to &quot;V1.0&quot;</TD></TR>
				<TR><TD PORT="reserved_pos">8</TD><TD PORT="reserved_size">4</TD><TD>u4le</TD><TD PORT="reserved_type">reserved</TD></TR>
				<TR><TD PORT="resource_count_pos">12</TD><TD PORT="resource_count_size">4</TD><TD>u4le</TD><TD PORT="resource_count_type">resource_count</TD></TR>
				<TR><TD PORT="offset_to_resource_table_pos">16</TD><TD PORT="offset_to_resource_table_size">4</TD><TD>u4le</TD><TD PORT="offset_to_resource_table_type">offset_to_resource_table</TD></TR>
			</TABLE>>];
			rim_header__inst__has_resources [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">id</TD><TD BGCOLOR="#E0FFE0">value</TD></TR>
				<TR><TD>has_resources</TD><TD>resource_count &gt; 0</TD></TR>
			</TABLE>>];
		}
	}
	rim__seq:header_type -> rim_header__seq [style=bold];
	rim__seq:extended_header_type -> rim_extended_header__seq [style=bold];
	rim__seq:resource_entry_table_type -> resource_entry_table__seq [style=bold];
	rim_header__seq:resource_count_type -> rim__seq:resource_entry_table__if [color="#404040"];
	resource_entry__seq:offset_to_data_type -> resource_entry__inst__data:data_pos [color="#404040"];
	resource_entry__seq:resource_size_type -> resource_entry__inst__data:data__repeat [color="#404040"];
	resource_entry_table__seq:entries_type -> resource_entry__seq [style=bold];
	rim_header__seq:resource_count_type -> resource_entry_table__seq:entries__repeat [color="#404040"];
	rim_header__seq:resource_count_type -> rim_header__inst__has_resources [color="#404040"];
}
