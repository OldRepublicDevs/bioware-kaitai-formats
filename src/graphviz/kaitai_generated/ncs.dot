digraph {
	rankdir=LR;
	node [shape=plaintext];
	subgraph cluster__ncs {
		label="Ncs";
		graph[style=dotted];

		ncs__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
			<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
			<TR><TD PORT="file_type_pos">0</TD><TD PORT="file_type_size">4</TD><TD>str(ASCII)</TD><TD PORT="file_type_type">file_type</TD></TR>
			<TR><TD COLSPAN="4" PORT="file_type__valid">must be equal to &quot;NCS &quot;</TD></TR>
			<TR><TD PORT="file_version_pos">4</TD><TD PORT="file_version_size">4</TD><TD>str(ASCII)</TD><TD PORT="file_version_type">file_version</TD></TR>
			<TR><TD COLSPAN="4" PORT="file_version__valid">must be equal to &quot;V1.0&quot;</TD></TR>
			<TR><TD PORT="size_marker_pos">8</TD><TD PORT="size_marker_size">1</TD><TD>u1</TD><TD PORT="size_marker_type">size_marker</TD></TR>
			<TR><TD COLSPAN="4" PORT="size_marker__valid">must be equal to 66</TD></TR>
			<TR><TD PORT="file_size_pos">9</TD><TD PORT="file_size_size">4</TD><TD>u4be</TD><TD PORT="file_size_type">file_size</TD></TR>
			<TR><TD PORT="instructions_pos">13</TD><TD PORT="instructions_size">...</TD><TD>Instruction</TD><TD PORT="instructions_type">instructions</TD></TR>
			<TR><TD COLSPAN="4" PORT="instructions__repeat">repeat until _io.pos &gt;= file_size</TD></TR>
		</TABLE>>];
		subgraph cluster__instruction {
			label="Ncs::Instruction";
			graph[style=dotted];

			instruction__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="opcode_pos">0</TD><TD PORT="opcode_size">1</TD><TD>u1</TD><TD PORT="opcode_type">opcode</TD></TR>
				<TR><TD PORT="qualifier_pos">1</TD><TD PORT="qualifier_size">1</TD><TD>u1</TD><TD PORT="qualifier_type">qualifier</TD></TR>
				<TR><TD PORT="arguments_pos">2</TD><TD PORT="arguments_size">1</TD><TD>u1</TD><TD PORT="arguments_type">arguments</TD></TR>
				<TR><TD COLSPAN="4" PORT="arguments__repeat">repeat until _io.pos &gt;= _io.size</TD></TR>
			</TABLE>>];
		}
	}
	ncs__seq:instructions_type -> instruction__seq [style=bold];
	ncs__seq:file_size_type -> ncs__seq:instructions__repeat [color="#404040"];
}
