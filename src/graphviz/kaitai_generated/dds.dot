digraph {
	rankdir=LR;
	node [shape=plaintext];
	subgraph cluster__dds {
		label="Dds";
		graph[style=dotted];

		dds__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
			<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
			<TR><TD PORT="magic_pos">0</TD><TD PORT="magic_size">4</TD><TD>str(ASCII)</TD><TD PORT="magic_type">magic</TD></TR>
			<TR><TD COLSPAN="4" PORT="magic__valid">must be any of &quot;DDS &quot;, &quot;    &quot;</TD></TR>
			<TR><TD PORT="header_pos">4</TD><TD PORT="header_size">124</TD><TD>DdsHeader</TD><TD PORT="header_type">header</TD></TR>
			<TR><TD COLSPAN="4" PORT="header__if">if magic == &quot;DDS &quot;</TD></TR>
			<TR><TD PORT="bioware_header_pos">128</TD><TD PORT="bioware_header_size">20</TD><TD>BiowareDdsHeader</TD><TD PORT="bioware_header_type">bioware_header</TD></TR>
			<TR><TD COLSPAN="4" PORT="bioware_header__if">if magic != &quot;DDS &quot;</TD></TR>
			<TR><TD PORT="pixel_data_pos">148</TD><TD PORT="pixel_data_size">1</TD><TD>u1</TD><TD PORT="pixel_data_type">pixel_data</TD></TR>
			<TR><TD COLSPAN="4" PORT="pixel_data__repeat">repeat to end of stream</TD></TR>
		</TABLE>>];
		subgraph cluster__bioware_dds_header {
			label="Dds::BiowareDdsHeader";
			graph[style=dotted];

			bioware_dds_header__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="width_pos">0</TD><TD PORT="width_size">4</TD><TD>u4le</TD><TD PORT="width_type">width</TD></TR>
				<TR><TD PORT="height_pos">4</TD><TD PORT="height_size">4</TD><TD>u4le</TD><TD PORT="height_type">height</TD></TR>
				<TR><TD PORT="bytes_per_pixel_pos">8</TD><TD PORT="bytes_per_pixel_size">4</TD><TD>u4le</TD><TD PORT="bytes_per_pixel_type">bytes_per_pixel</TD></TR>
				<TR><TD PORT="data_size_pos">12</TD><TD PORT="data_size_size">4</TD><TD>u4le</TD><TD PORT="data_size_type">data_size</TD></TR>
				<TR><TD PORT="unused_float_pos">16</TD><TD PORT="unused_float_size">4</TD><TD>f4le</TD><TD PORT="unused_float_type">unused_float</TD></TR>
			</TABLE>>];
		}
		subgraph cluster__ddpixelformat {
			label="Dds::Ddpixelformat";
			graph[style=dotted];

			ddpixelformat__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="size_pos">0</TD><TD PORT="size_size">4</TD><TD>u4le</TD><TD PORT="size_type">size</TD></TR>
				<TR><TD COLSPAN="4" PORT="size__valid">must be equal to 32</TD></TR>
				<TR><TD PORT="flags_pos">4</TD><TD PORT="flags_size">4</TD><TD>u4le</TD><TD PORT="flags_type">flags</TD></TR>
				<TR><TD PORT="fourcc_pos">8</TD><TD PORT="fourcc_size">4</TD><TD>str(ASCII)</TD><TD PORT="fourcc_type">fourcc</TD></TR>
				<TR><TD PORT="rgb_bit_count_pos">12</TD><TD PORT="rgb_bit_count_size">4</TD><TD>u4le</TD><TD PORT="rgb_bit_count_type">rgb_bit_count</TD></TR>
				<TR><TD PORT="r_bit_mask_pos">16</TD><TD PORT="r_bit_mask_size">4</TD><TD>u4le</TD><TD PORT="r_bit_mask_type">r_bit_mask</TD></TR>
				<TR><TD PORT="g_bit_mask_pos">20</TD><TD PORT="g_bit_mask_size">4</TD><TD>u4le</TD><TD PORT="g_bit_mask_type">g_bit_mask</TD></TR>
				<TR><TD PORT="b_bit_mask_pos">24</TD><TD PORT="b_bit_mask_size">4</TD><TD>u4le</TD><TD PORT="b_bit_mask_type">b_bit_mask</TD></TR>
				<TR><TD PORT="a_bit_mask_pos">28</TD><TD PORT="a_bit_mask_size">4</TD><TD>u4le</TD><TD PORT="a_bit_mask_type">a_bit_mask</TD></TR>
			</TABLE>>];
		}
		subgraph cluster__dds_header {
			label="Dds::DdsHeader";
			graph[style=dotted];

			dds_header__seq [label=<<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
				<TR><TD BGCOLOR="#E0FFE0">pos</TD><TD BGCOLOR="#E0FFE0">size</TD><TD BGCOLOR="#E0FFE0">type</TD><TD BGCOLOR="#E0FFE0">id</TD></TR>
				<TR><TD PORT="size_pos">0</TD><TD PORT="size_size">4</TD><TD>u4le</TD><TD PORT="size_type">size</TD></TR>
				<TR><TD COLSPAN="4" PORT="size__valid">must be equal to 124</TD></TR>
				<TR><TD PORT="flags_pos">4</TD><TD PORT="flags_size">4</TD><TD>u4le</TD><TD PORT="flags_type">flags</TD></TR>
				<TR><TD PORT="height_pos">8</TD><TD PORT="height_size">4</TD><TD>u4le</TD><TD PORT="height_type">height</TD></TR>
				<TR><TD PORT="width_pos">12</TD><TD PORT="width_size">4</TD><TD>u4le</TD><TD PORT="width_type">width</TD></TR>
				<TR><TD PORT="pitch_or_linear_size_pos">16</TD><TD PORT="pitch_or_linear_size_size">4</TD><TD>u4le</TD><TD PORT="pitch_or_linear_size_type">pitch_or_linear_size</TD></TR>
				<TR><TD PORT="depth_pos">20</TD><TD PORT="depth_size">4</TD><TD>u4le</TD><TD PORT="depth_type">depth</TD></TR>
				<TR><TD PORT="mipmap_count_pos">24</TD><TD PORT="mipmap_count_size">4</TD><TD>u4le</TD><TD PORT="mipmap_count_type">mipmap_count</TD></TR>
				<TR><TD PORT="reserved1_pos">28</TD><TD PORT="reserved1_size">4</TD><TD>u4le</TD><TD PORT="reserved1_type">reserved1</TD></TR>
				<TR><TD COLSPAN="4" PORT="reserved1__repeat">repeat 11 times</TD></TR>
				<TR><TD PORT="pixel_format_pos">72</TD><TD PORT="pixel_format_size">32</TD><TD>Ddpixelformat</TD><TD PORT="pixel_format_type">pixel_format</TD></TR>
				<TR><TD PORT="caps_pos">104</TD><TD PORT="caps_size">4</TD><TD>u4le</TD><TD PORT="caps_type">caps</TD></TR>
				<TR><TD PORT="caps2_pos">108</TD><TD PORT="caps2_size">4</TD><TD>u4le</TD><TD PORT="caps2_type">caps2</TD></TR>
				<TR><TD PORT="caps3_pos">112</TD><TD PORT="caps3_size">4</TD><TD>u4le</TD><TD PORT="caps3_type">caps3</TD></TR>
				<TR><TD PORT="caps4_pos">116</TD><TD PORT="caps4_size">4</TD><TD>u4le</TD><TD PORT="caps4_type">caps4</TD></TR>
				<TR><TD PORT="reserved2_pos">120</TD><TD PORT="reserved2_size">4</TD><TD>u4le</TD><TD PORT="reserved2_type">reserved2</TD></TR>
			</TABLE>>];
		}
	}
	dds__seq:header_type -> dds_header__seq [style=bold];
	dds__seq:magic_type -> dds__seq:header__if [color="#404040"];
	dds__seq:bioware_header_type -> bioware_dds_header__seq [style=bold];
	dds__seq:magic_type -> dds__seq:bioware_header__if [color="#404040"];
	dds_header__seq:pixel_format_type -> ddpixelformat__seq [style=bold];
}
